@using ThamQuanTDMU.Models
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <script src="~/Content/TemplateAdmin/js/angular.min.js"></script>
    <link href="~/Content/TemplateAdmin/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/TemplateAdmin/css/bootstrap-responsive.min.css" rel="stylesheet" />
    <link href="~/Content/TemplateAdmin/css/style.css" rel="stylesheet" />
    <link href="~/Content/TemplateAdmin/css/style-responsive.css" rel="stylesheet" />
    <link href="~/Content/TemplateAdmin/css/AdminStyle.css" rel="stylesheet" />
    <script src="~/Content/TemplateAdmin/js/tinymce/tinymce.min.js"></script>
        <script>tinymce.init({ selector: 'textarea' });</script>
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800&subset=latin,cyrillic-ext,latin-ext' rel='stylesheet' type='text/css'>
    <title>@ViewBag.Title</title>
    <style>
        body {
            font-family: 'Times New Roman';
        }
    </style>
</head>
<body>
    <!-- start: Header -->
    <div class="navbar">
        <div class="navbar-inner">
            <div class="container-fluid">
                <a class="btn btn-navbar" data-toggle="collapse" data-target=".top-nav.nav-collapse,.sidebar-nav.nav-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <a class="brand" href="@Url.Action("Index","QuanLyMenu")"><span>TDMU</span></a>

                <!-- start: Header Menu -->
                <div class="nav-no-collapse header-nav">
                    <ul class="nav pull-right">
                        <!-- start: User Dropdown -->
                        <li class="dropdown">
                            <a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
                                <i class="halflings-icon white user"></i> 
                                <span class="caret"></span>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="dropdown-menu-title">
                                    <span>Tài khoản</span>
                                </li>
                                <li><a href="#"><i class="halflings-icon user"></i> @Session["TaiKhoan"].ToString()</a></li>
                                <li><a href="@Url.Action("DangXuat","Login")"><i class="halflings-icon off"></i> Đăng xuất</a></li>
                            </ul>
                        </li>
                        <!-- end: User Dropdown -->
                    </ul>
                </div>
                <!-- end: Header Menu -->

            </div>
        </div>
    </div>
    <!-- start: Header -->

    <div class="container-fluid-full">
        <div class="row-fluid">

            <!-- start: Main Menu -->
            <div id="sidebar-left" class="span2">
                <div class="nav-collapse sidebar-nav">
                    <ul class="nav nav-tabs nav-stacked main-menu">
                        <li><a href="@Url.Action("Index","QuanLyMenu")"><i class="icon-book"></i><span class="hidden-tablet">Quản lý Menu</span></a></li>
                        <li><a href="@Url.Action("Index","QuanLyBaiDang")"><i class="icon-book"></i><span class="hidden-tablet">Quản lý bài đăng</span></a></li>
                        <li><a href="@Url.Action("Index","QuanLyVideoGioiThieu")"><i class="icon-book"></i><span class="hidden-tablet">Quản lý video giới thiệu</span></a></li>
                        <li><a href="@Url.Action("Index","QuanLyAdmin")"><i class="icon-book"></i><span class="hidden-tablet">Quản lý Admin</span></a></li>
                        <li><a href="@Url.Action("Index","QuanLyHinhAnhThamQuan")"><i class="icon-book"></i><span class="hidden-tablet">Quản lý hình ảnh tham quan</span></a></li>
                        <li><a href="@Url.Action("Index","QuanLyMoiTruongHocTap")"><i class="icon-book"></i><span class="hidden-tablet">Quản lý Môi trường học tập</span></a></li>
                        <li><a href="@Url.Action("Index","QuanLyNghienCuuKhoaHoc")"><i class="icon-book"></i><span class="hidden-tablet">Quản lý Hoạt động nghiên cứu</span></a></li>
                        <li><a href="@Url.Action("Index","QuanLyLienKetTruong")"><i class="icon-book"></i><span class="hidden-tablet">Quản lý liên kết trường</span></a></li>
                        <li><a href="@Url.Action("Index","QuanLyFooter")"><i class="icon-book"></i><span class="hidden-tablet">Quản lý footer</span></a></li>
                    </ul>
                </div>
            </div>
            <!-- end: Main Menu -->

            <noscript>
                <div class="alert alert-block span10">
                    <h4 class="alert-heading">Warning!</h4>
                    <p>You need to have <a href="http://en.wikipedia.org/wiki/JavaScript" target="_blank">JavaScript</a> enabled to use this site.</p>
                </div>
            </noscript>

            <!-- start: Content -->
            <div id="content" class="span10">

                @RenderBody()
            </div><!--/.fluid-container-->
            <!-- end: Content -->
        </div><!--/#content.span10-->
    </div><!--/fluid-row-->

    <div class="modal hide fade" id="myModal">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">×</button>
            <h3>Settings</h3>
        </div>
        <div class="modal-body">
            <p>Here settings can be configured...</p>
        </div>
        <div class="modal-footer">
            <a href="#" class="btn" data-dismiss="modal">Close</a>
            <a href="#" class="btn btn-primary">Save changes</a>
        </div>
    </div>

    <div class="clearfix"></div>

    <footer>

        <p>
            <span style="text-align:left;float:left">
                &copy;2017 Ứng dụng hỗ trợ tham quan trường Đại học Thủ Dầu Một
            </span>
        </p>

    </footer>

    <!-- start: JavaScript-->

    <script src="~/Content/TemplateAdmin/js/jquery-1.9.1.min.js"></script>
    <script src="~/Content/TemplateAdmin/js/jquery-migrate-1.0.0.min.js"></script>

    <script src="~/Content/TemplateAdmin/js/jquery-ui-1.10.0.custom.min.js"></script>

    <script src="~/Content/TemplateAdmin/js/jquery.ui.touch-punch.js"></script>

    <script src="~/Content/TemplateAdmin/js/modernizr.js"></script>

    <script src="~/Content/TemplateAdmin/js/bootstrap.min.js"></script>

    <script src="~/Content/TemplateAdmin/js/jquery.cookie.js"></script>

    <script src='~/Content/TemplateAdmin/js/fullcalendar.min.js'></script>

    <script src='~/Content/TemplateAdmin/js/jquery.dataTables.min.js'></script>

    <script src="~/Content/TemplateAdmin/js/excanvas.js"></script>
    <script src="~/Content/TemplateAdmin/js/jquery.flot.js"></script>
    <script src="~/Content/TemplateAdmin/js/jquery.flot.pie.js"></script>
    <script src="~/Content/TemplateAdmin/js/jquery.flot.stack.js"></script>
    <script src="~/Content/TemplateAdmin/js/jquery.flot.resize.min.js"></script>

    <script src="~/Content/TemplateAdmin/js/jquery.chosen.min.js"></script>

    <script src="~/Content/TemplateAdmin/js/jquery.uniform.min.js"></script>

    <script src="~/Content/TemplateAdmin/js/jquery.cleditor.min.js"></script>

    <script src="~/Content/TemplateAdmin/js/jquery.noty.js"></script>

    <script src="~/Content/TemplateAdmin/js/jquery.elfinder.min.js"></script>

    <script src="~/Content/TemplateAdmin/js/jquery.raty.min.js"></script>

    <script src="~/Content/TemplateAdmin/js/jquery.iphone.toggle.js"></script>

    <script src="~/Content/TemplateAdmin/js/jquery.uploadify-3.1.min.js"></script>

    <script src="~/Content/TemplateAdmin/js/jquery.gritter.min.js"></script>

    <script src="~/Content/TemplateAdmin/js/jquery.imagesloaded.js"></script>

    <script src="~/Content/TemplateAdmin/js/jquery.masonry.min.js"></script>

    <script src="~/Content/TemplateAdmin/js/jquery.knob.modified.js"></script>

    <script src="~/Content/TemplateAdmin/js/jquery.sparkline.min.js"></script>

    <script src="~/Content/TemplateAdmin/js/counter.js"></script>

    <script src="~/Content/TemplateAdmin/js/retina.js"></script>

    <script src="~/Content/TemplateAdmin/js/custom.js"></script>
    <!-- end: JavaScript-->

</body>
</html>